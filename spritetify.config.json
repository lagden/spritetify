{
  "id": "my_app_%s",
  "multipass": true,
  "plugins": [
    {
      "name": "removeTitle"
    },
    {
      "name": "removeAttrs",
      "params": {
        "attrs": "(fill|stroke)"
      }
    },
    {
      "name": "sortAttrs",
      "params": {
        "xmlnsOrder": "alphabetical"
      }
    }
  ]
}
